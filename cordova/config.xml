<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mytrainr.app" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
		<icon src="../dist/assets/images/icon.png" />
	 <name>Mytrainr</name>
    <description>
        Het video-platform voor de schaatssport
	</description>
	<access origin="https://codepush.azurewebsites.net/" />
	<author email="mathijs@code.rehab" href="http://code.rehab">
		Code rehab Team
	</author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
</widget>